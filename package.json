{
  "name": "chaingraph-indexer",
  "version": "1.0.0",
  "description": "Chaingraph indexer",
  "main": "index.js",
  "repository": "git@github.com:chaingraph/chaingraph-indexer.git",
  "author": "blockmatic.io",
  "license": "MIT",
  "scripts": {
    "dev": "env-cmd ts-node-dev --no-notify --no-deps --respawn src/index",
    "build": "tsc -b",
    "lint": "eslint --ignore-path .gitignore \"**/*.+(js|ts|tsx)\"",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|ts|tsx)\"",
    "seed": "hasura seeds apply --project src/hasura ",
    "gen": "rimraf src/generated && graphql-codegen --config codegen.yml -r dotenv/config",
    "doctoc": "doctoc ./README.md --github"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@blockmatic/eslint-config": "^2.0.0",
    "@blockmatic/prettier-config": "^2.0.0",
    "@types/bluebird": "^3.5.36",
    "@types/lodash.chunk": "^4.2.6",
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.uniq": "^4.5.6",
    "@types/lodash.uniqby": "^4.7.6",
    "@types/node": "^16.9.4",
    "@types/text-encoding": "^0.0.36",
    "@types/ws": "^7.4.7",
    "cpy-cli": "^3.1.1",
    "doctoc": "^2.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "7",
    "hasura-cli": "^2.0.8",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "prisma": "3.0.2",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@blockmatic/eosio-ship-reader": "^0.4.3",
    "@chaingraph.io/hasura-client": "^1.4.1",
    "@dfuse/client": "^0.3.20",
    "@eoscafe/hyperion": "^3.2.9",
    "@eosrio/node-abieos": "^2.1.1",
    "@greymass/eosio": "^0.5.0",
    "@prisma/client": "3.0.2",
    "bluebird": "^3.7.2",
    "env-cmd": "^10.1.0",
    "env-var": "^7.0.1",
    "eos-common": "^0.8.1",
    "eosjs": "^22.1.0",
    "eosjs-account-name": "^2.2.1",
    "graphql": "^15.6.0",
    "graphql-request": "3.5.0",
    "hasura-cli": "^2.0.8",
    "isomorphic-fetch": "^3.0.0",
    "lodash.chunk": "^4.2.0",
    "lodash.omit": "^4.5.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "node-fetch": "^3.0.0",
    "p-queue": "^7.1.0",
    "p-throttle": "4.1.1",
    "pg-promise": "^10.11.0",
    "rxjs": "^7.3.0",
    "text-encoding": "^0.7.0",
    "tslog": "^3.2.2",
    "winston": "^3.3.3",
    "ws": "^8.2.2"
  }
}
